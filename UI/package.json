{
  "name": "react-boilerplate",
  "version": "1.1",
  "description": "Boilerplate for building web apps using React, Express and Node.js",
  "main": "renderer.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "app": "nodemon ../API/app.js",
    "dev": "concurrently \"npm run app\" \"npm start\""
  },
  "author": "Leon Schwarz",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.2.2",
    "css-loader": "^6.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.0.0",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.7.3",
    "nodemon": "2.0.22"
  }
}
